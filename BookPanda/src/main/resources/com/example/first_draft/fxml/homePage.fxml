<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/25"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.first_draft.controller.HomePageController"
            stylesheets="@../css/homeWithGenre.css">

    <children>
        <ScrollPane fx:id="scrollPane" fitToWidth="true"
                    AnchorPane.topAnchor="0.0"
                    AnchorPane.bottomAnchor="0.0"
                    AnchorPane.leftAnchor="0.0"
                    AnchorPane.rightAnchor="0.0">
            <content>
                <VBox fx:id="vbox" spacing="10.0" />
            </content>
        </ScrollPane>
    </children>
</AnchorPane>
